{
  "name": "this.might.be/offensive - open pix",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "open a bunch of pix from tmbo at once",
  "homepage_url": "http://github.com/andytuba/tmbo/"
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
        "matches": "https://thismight.be/offensive/?c=hof&t=today",
	"js": [ "src/open-top-pix.js" ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "https://thismight.be/offensive/*"
  ]
}
